<div class= "container">
<div class = "row">
     <div class="col-md d-flex justify-content-center">
         <h3>Movie</h3>
      </div>
      <div class="col-md d-flex justify-content-center">
         <h3> Rating</h3>
      </div>
</div>
<%object.each do |movie| %>
<div class = "row">
    <div class = "col-md d-flex justify-content-center">
    <%= link_to "#{movie.title} (#{movie.release_year})", user_movie_path(@user, movie.id) %>
    </div>
    <div class = "col-md d-flex justify-content-center">
        <p>Vote Average <%= movie.vote_average %></p>
    </div>
</div>
<% end %>
</div>